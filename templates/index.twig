{% extends '_snippets/_head.twig' %}

{% block content %}
<body id="home">

<nav>
    <a href="index.html">
        <img src="static/assets/Dolfijn%201.png" alt="logo">
    </a>
    <div class="menu-items">
        <a href="over-ons.twig">Over ons</a>
        <a href="duiken.twig">Duiken</a>
        <a href="contact.twig">Contact</a>
        <a href="login.twig">
            <button>Login</button>
        </a>
    </div>
</nav>
{% set homeImage = entry.homeImage.one() %}
<div id="header-home" style="background-image:url('{{ homeImage.url }}')">
    <h1>{{  entry.titelH1 }}</h1>
</div>

<main>
    <section id="welkomhome">
        <div class="tekst">
            <h2>Welkom bij duikclub Dolfijn!</h2>
            <p>{{ entry.inleidendeTekstHome }}</p>
            <a href="lessen-initiatie.html">
                <button>Gratis duikinitiatie</button>
            </a>
        </div>
        <div class="fotos">
            <img class="foto-top" src="static/assets/Mask%20group.png" alt="foto">
            <img class="foto-mid" src="static/assets/Mask%20group2.png" alt="foto">
            <img class="foto-bot" src="static/assets/Mask%20group3.png" alt="foto">
        </div>
    </section>
    <section id="aanbodhome">
        <h2>Ons aanbod</h2>
        <div>
            <a href="lessen-initiatie.html">
                <button>Initiatie</button>
            </a>
            <a href="duiken.html">
                <button>Clubduiken</button>
            </a>
            <a href="duiksport.html">
                <button>Trainingen</button>
            </a>
        </div>
    </section>
    <section class="dank">
        <h2>Met dank aan</h2>
        <div>
            <a href="https://www.limos-vzw.be/" target="_blank"><img src="static/assets/Limos_full%201.png" alt="partner"></a>
            <a href="https://nelos.be/" target="_blank"><img src="static/assets/Nelos_full%201.png" alt="partner"></a>
            <a href="https://www.cmas.org/" target="_blank"><img src="static/assets/CMAS_BEFOS_full%201.png" alt="partner"></a>
        </div>
    </section>
</main>
<footer>
    <section class="gegevensfooter">
        <div class="border">
            <img src="static/assets/Dolfijn_full%201.png" alt="logo">
        </div>
        <div class="border">
            <p><b>Met dank aan:</b></p>
            <a href="https://www.limos-vzw.be/" target="_blank">Limos</a>
            <a href="https://nelos.be/" target="_blank">Nelos</a>
            <a href="https://www.cmas.org/" target="_blank">CMAS</a>
        </div>
        <div class="border">
            <div class="gegevensmeticonen"><img src="static/assets/icon%20_home_.png" alt="icon">
                <p>Eikenstraat 25<br>3740 Bilzen</p></div>
            <div class="gegevensmeticonen"><img src="static/assets/icon%20_envelope%20closed_.png" alt="icon">
                <p>email@email.com</p></div>
            <div class="gegevensmeticonen"><img src="static/assets/icon%20_phone_.png" alt="icon">
                <p>+32 456 78 91</p></div>
        </div>
        <div>
            <img src="static/assets/icon%20_web_.png" alt="icon">
            <div class="socialsfooter">
                <a href="https://www.facebook.com/DuikclubDolfijn/" target="_blank"><img src="static/assets/icon%20_facebook_.png" alt="icon"></a>
                <a href="#"><img src="static/assets/icon%20_instagram%20alt_.png" alt="icon"></a>
                <a href="#"><img src="static/assets/icon%20_social%20linkedin_.png" alt="icon"></a>
            </div>
        </div>
    </section>
    <section class="balkfooter">
        <a href="#">Regelement</a>
        <a href="#">Algemene gebruiksvoorwaarden</a>
        <a href="#">privacyverklaring</a>
    </section>
</footer>
</body>

{% endblock %}
